            data = data.data;
            console.log(data);  
        
            var ctx = $("#chart_ts_p2tl_unit canvas");
            ctx.height = 250;
            new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: data.singkatan,
                    datasets: [
                        {
                            label: "Merah",
                            type: "bar",
                            stack: 'Kategori',
                            backgroundColor: "#ff5722",
                            backgroundColorHover: "#ff5722",
                            data: data.merah_rpts_unit,
                        },
                        {
                            label: "Kuning",
                            type: "bar",
                            stack: 'Kategori',
                            backgroundColor: "#ffc107",
                            backgroundColorHover: "#ffc107",
                            data: data.kuning_rpts_unit,
                        },
                        {
                            label: "Hijau",
                            type: "bar",
                            stack: 'Kategori',
                            backgroundColor: "#4caf50",
                            backgroundColorHover: "#4caf50",
                            data: data.hijau_rpts_unit,
                        },
                    ]
                },
                plugins:
                {      
                    datasets: [],                                                                                                                                                                             
                    getData(labels, datasets) {                                                                                                                                                      
                        const sum = [];                                                                                                                                                              
                        for (let i = 0; i < datasets[0].length; i++) {                                                                                                                                   
                            sum.push({                                                                                                                                                               
                                label: labels[i],                                                                                                                                                    
                                data: datasets.map(e => e[i]),                                                                                                                                       
                                    get sum() { // ES6 - getter                                                                                                                                      
                                        return this.data.reduce((a, b) => a + b);                                                                                                                    
                                    }                                                                                                                                                                
                            });                                                                                                                                                                      
                        }                                                                                                                                                                            
                        return sum;                                                                                                                                                                  
                    },                                                                                                                                                                               
                    beforeInit(chart) { 
                        chart.data.datasets.forEach((dataset, datasetIndex) => {                                                                                                                     
                            this.datasets.push(dataset.data);                                                                                                                                        
                        });                                                                                                                                                                          
                        const data_store = this.getData(chart.data.labels, this.datasets).sort((a,b) => b.sum - a.sum);                                                                              

                        data_store.forEach((d,i) => {                                                                                                                                                
                            chart.data.labels[i] = d.label;                                                                                                                                          
                            d.data.forEach((v, vi) => {                                                                                                                                              
                                chart.data.datasets[vi].data[i] = v;                                                                                                                                 
                            });                                                                                                                                                                      
                        });                                                                                                                                                            
                    }                                                                                                                                                                                
                },
                options: {
                    responsive: true, 
                    maintainAspectRatio: false, 
                    barRadius: 2,
                    title: {
                        display: false,
                        text: 'Rekap Saldo TS P2TL'
                    },
                    tooltips: {
                       mode: 'label',
                       bodyFontSize: 10,
                       callbacks: {
                          label: function(t, d) {
                             var dstLabel = d.datasets[t.datasetIndex].label;
                             var yLabel = t.yLabel;
                             if(isNaN(yLabel))
                                yLabel = 0;
                             return dstLabel + ': Rp. ' + Format.to(yLabel);
                          }
                       }
                    },
                    legend: {
                        position: 'bottom',
                        display: false,
                        labels: {
                            fontSize: 10,
                        },
                    },
                    plugins: {
                        labels: []
                    },
                    scales: {
                        yAxes: [{
                            display: false,
                            stacked: true,
                            gridLines: {
                                display: false,
                                drawBorder: false
                            },
                            ticks: {
